#include <stdlib.h>
#include <stdio.h>
#include <string.h>

void print(char *palavra){
    int i;
    char *p = palavra;
    for(i = 0; i < strlen(palavra); i++){
        printf("%c",*p);
        p++;
    }
}
int main()
{   
    char palavra[20] = "minha nossa senhora";
    print(palavra);

    return 0;
}
